        {
            "name": "Amy Stephen",
            "email": "AmyStephen@molajo.org",
            "homepage": "http://AmyStephen.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Molajo/User/issues",
